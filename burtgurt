javascript:(function(){const divs=document.querySelectorAll('.live-streams-monitoring-point_popular__x1FQU');let lastClickedDiv=null;divs.forEach(div=>{if(!lastClickedDiv){div.click();lastClickedDiv=div;}});const observer=new MutationObserver(()=>{divs.forEach(div=>{if(div!==lastClickedDiv){div.click();lastClickedDiv=div;}});});observer.observe(document.body,{childList:true,subtree:true});})();
